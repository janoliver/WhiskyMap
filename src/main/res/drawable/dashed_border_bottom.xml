<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
            android:bottom="1dp"
            android:left="-1dp"
            android:right="-1dp"
            android:top="-1dp">
        <shape>
            <solid android:color="#00000000"/>
            <stroke
                    android:width="1dp"
                    android:color="#eeeeee"
                    android:dashWidth="4dp"
                    android:dashGap="4dp"/>
            <padding
                    android:left="10dp"
                    android:right="10dp"
                    android:top="10dp"
                    android:bottom="10dp"/>
        </shape>
    </item>
</layer-list>